{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "strict": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "rootDir": "../",
      "outDir": "../dist/server",
      "baseUrl": "../",
      "paths": {
        "@/*": ["./*"]
      },
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true
    },
    "include": ["./**/*.ts"],
  }
  